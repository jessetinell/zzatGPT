{
  "build": {
    "devPath": "http://localhost:5173",
    "beforeDevCommand": "cd frontend && yarn dev",
    "distDir": "../frontend/dist",
    "beforeBuildCommand": "cd frontend && yarn build"
  },
  "package": {
    "productName": "zzatGPT",
    "version": "1.0.6"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "category": "Productivity",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.jessetinell.dev",
      "shortDescription": "Blazingly fast and smart LLM chat",
      "longDescription": "",
      "resources": [],
      "targets": "all",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "systemTray": {
      "iconPath": "system-tray-icon.png",
      "iconAsTemplate": true
    },
    "windows": [
      {
        "title": "zzatGPT",
        "width": 630,
        "height": 70,
        "resizable": false,
        "fullscreen": false,
        "alwaysOnTop": true,
        "visible": false,
        "decorations": false,
        "center": true
      }
    ],
    "updater": {
      "active": false
    }
  }
}